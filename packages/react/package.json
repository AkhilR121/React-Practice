{
  "name": "@ti/react",
  "version": "1.0.0",
  "description": "library for react apps",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "sideEffects": false,
  "files": [
    "dist/**"
  ],
  "keywords": [],
  "scripts": {
    "upd": "pnpm ncu -u",
    "build": "tsup src/index.ts --format esm --dts",
    "dev": "tsup src/index.ts --format esm --watch --dts",
    "types:dev": "tsc --noEmit -p . --watch",
    "types": "tsc --noEmit -p .",
    "lint": "eslint --fix --max-warnings=0 --cache src --ext .ts,.tsx",
    "clean": "rm -rf .turbo && rm -rf node_modules && rm -rf dist"
  },
  "dependencies": {
    "@tanstack/react-query": "4.29.12",
    "@ti/core": "workspace:*",
    "immer": "10.0.2",
    "jotai": "2.1.0",
    "query-string": "8.1.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-error-boundary": "4.0.9",
    "react-hot-toast": "2.4.1",
    "react-router-dom": "6.11.2",
    "redaxios": "0.5.1",
    "tiny-invariant": "1.3.1",
    "tiny-warning": "1.0.3",
    "tslib": "2.5.3",
    "use-immer": "0.9.0",
    "wretch": "2.5.2",
    "zod": "3.21.4"
  },
  "devDependencies": {
    "@tsconfig/esm": "1.0.3",
    "@tsconfig/strictest": "2.0.1",
    "@types/node": "20.2.5",
    "@types/react": "18.2.8",
    "@types/react-dom": "18.2.4",
    "cross-env": "7.0.3",
    "eslint": "8.42.0",
    "eslint-config-ti": "workspace:*",
    "prettier": "2.8.8",
    "tsup": "6.7.0",
    "typescript": "5.1.3"
  }
}
