{
  "name": "@ti/client",
  "version": "1.0.0",
  "description": "library for react apps",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "sideEffects": false,
  "files": [
    "dist/**"
  ],
  "keywords": [],
  "scripts": {
    "build": "tsup src/index.ts --format esm,cjs --dts",
    "dev": "tsup src/index.ts --format esm,cjs --watch --dts",
    "types:dev": "tsc --noEmit -p . --watch",
    "types": "tsc --noEmit -p .",
    "lint": "eslint --fix --max-warnings=0 --cache src --ext .ts,.tsx",
    "lint:dev": "cross-env TIMING=1 pnpm lint --quiet --fix",
    "test": "vitest run",
    "test:dev": "vitest dev",
    "coverage": "vitest run --coverage",
    "clean": "rm -rf .turbo && rm -rf node_modules && rm -rf dist"
  },
  "dependencies": {
    "immer": "10.0.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "tiny-invariant": "1.3.1",
    "tiny-warning": "1.0.3",
    "tslib": "2.5.0",
    "zod": "3.21.4"
  },
  "devDependencies": {
    "@tsconfig/esm": "1.0.3",
    "@tsconfig/strictest": "2.0.1",
    "@types/node": "18.16.0",
    "@types/react": "18.0.38",
    "@types/react-dom": "18.0.11",
    "cross-env": "7.0.3",
    "eslint": "8.39.0",
    "eslint-config-ti": "workspace:*",
    "prettier": "2.8.8",
    "tsup": "6.7.0",
    "typescript": "5.0.4",
    "vitest": "0.30.1"
  }
}
