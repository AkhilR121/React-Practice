{
  "name": "@ti/fastify-api",
  "version": "1.0.0",
  "description": "This project was bootstrapped with Fastify-CLI.",
  "main": "src/server.ts",
  "scripts": {
    "dev": "tsx src/server.ts",
    "lint": "eslint --fix --cache --ext .ts,.tsx src",
    "types": "tsc --noEmit -p .",
    "format": "pnpm prettier --write --ignore-unknown .",
    "types:dev": "tsc --noEmit -p . --watch",
    "cd": "turbo types lint"
  },
  "dependencies": {
    "@fastify/autoload": "5.7.1",
    "@fastify/sensible": "5.2.0",
    "@fastify/type-provider-typebox": "3.0.0",
    "close-with-grace": "1.2.0",
    "dotenv": "16.0.3",
    "fastify": "4.15.0",
    "fastify-plugin": "4.5.0",
    "tiny-invariant": "1.3.1"
  },
  "devDependencies": {
    "@types/node": "18.15.11",
    "eslint-config-ti": "workspace:*",
    "fastify-tsconfig": "1.0.1",
    "prettier": "2.8.7",
    "tsx": "3.12.6",
    "typescript": "5.0.4"
  }
}
