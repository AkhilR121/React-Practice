{
  "name": "@ti/react-app",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "lint": "eslint --fix --cache --ext .ts,.tsx src",
    "test:dev": "vitest",
    "test": "vitest run --passWithNoTests",
    "cov": "vitest run --coverage",
    "cov:dev": "vitest watch --coverage",
    "types": "tsc --noEmit -p .",
    "format": "pnpm prettier --write --ignore-unknown .",
    "types:dev": "tsc --noEmit -p . --watch",
    "cd": "turbo types lint test"
  },
  "dependencies": {
    "react": "18.2.0",
    "react-dom": "18.2.0"
  },
  "devDependencies": {
    "@ti/tsconfig": "workspace:*",
    "@types/react": "18.0.35",
    "@types/react-dom": "18.0.11",
    "@vitejs/plugin-react-swc": "3.3.0",
    "eslint": "8.38.0",
    "eslint-config-ti": "workspace:*",
    "npm-check-updates": "16.10.8",
    "prettier": "2.8.7",
    "typescript": "5.0.4",
    "vite": "4.2.1",
    "vitest": "0.30.1"
  }
}
