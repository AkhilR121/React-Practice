{
  "name": "@ti/react-app",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "lint": "eslint --fix --cache --ext .ts,.tsx src",
    "test:dev": "vitest",
    "test": "vitest run --passWithNoTests",
    "cov": "vitest run --coverage",
    "cov:dev": "vitest watch --coverage",
    "types": "tsc --noEmit -p .",
    "format": "pnpm prettier --write --ignore-unknown .",
    "types:dev": "tsc --noEmit -p . --watch",
    "cd": "turbo types lint test"
  },
  "dependencies": {
    "@preact/signals": "1.1.3",
    "dayjs": "1.11.7",
    "immer": "9.0.21",
    "query-string": "8.1.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-error-boundary": "4.0.3",
    "react-router-dom": "6.10.0",
    "remeda": "1.12.0",
    "superjson": "1.12.2",
    "tiny-invariant": "1.3.1",
    "tiny-warning": "1.0.3",
    "tslib": "2.5.0",
    "use-immer": "0.9.0",
    "zod": "3.21.4"
  },
  "devDependencies": {
    "@faker-js/faker": "7.6.0",
    "@ti/tsconfig": "workspace:*",
    "@types/node": "18.15.11",
    "@types/react": "18.0.35",
    "@types/react-dom": "18.0.11",
    "@vitejs/plugin-react-swc": "3.3.0",
    "eslint": "8.38.0",
    "eslint-config-ti": "workspace:*",
    "npm-check-updates": "16.10.8",
    "prettier": "2.8.7",
    "rimraf": "5.0.0",
    "typescript": "5.0.4",
    "vite": "4.2.1",
    "vitest": "0.30.1"
  }
}
