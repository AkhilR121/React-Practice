    "version": "0.2.0",
    "configurations": [
        {
            "name": "Vite Debug",
            "request": "launch",
            "type": "chrome",
            "url": "http://localhost:3000",
            "webRoot": "${workspaceFolder}"
        },
      {
        "type": "chrome",
        "request": "launch",
        "name": "Parcel Debug",
        "url": "http://localhost:1234",
        "webRoot": "${workspaceFolder}",
        "sourceMapPathOverrides": {
          "../__parcel_source_root/*": "${webRoot}/*"
        }
      }
    ]
  }