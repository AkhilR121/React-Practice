{
  "name": "@ti/ts",
  "version": "1.0.0",
  "private": true,
  "main": "dist/index.js",
  "scripts": {
    "lint": "eslint --fix --cache --ext .ts,.tsx src",
    "dev": "vite",
    "test:dev": "vitest",
    "test": "vitest run --passWithNoTests",
    "cov": "vitest run --coverage",
    "cov:dev": "vitest watch --coverage",
    "types": "tsc --noEmit -p .",
    "format": "pnpm prettier --write --ignore-unknown .",
    "types:dev": "tsc --noEmit -p . --watch",
    "cd": "turbo types lint test"
  },
  "dependencies": {
    "@ti/core": "workspace:*",
    "immer": "10.0.1",
    "remeda": "1.13.0",
    "tiny-invariant": "1.3.1",
    "tiny-warning": "1.0.3",
    "tslib": "2.5.0",
    "zod": "3.21.4"
  },
  "devDependencies": {
    "@faker-js/faker": "7.6.0",
    "@total-typescript/ts-reset": "0.4.2",
    "@tsconfig/esm": "1.0.3",
    "@tsconfig/strictest": "2.0.1",
    "@types/node": "18.16.0",
    "@vitest/coverage-c8": "0.30.1",
    "eslint": "8.39.0",
    "eslint-config-ti": "workspace:*",
    "prettier": "2.8.8",
    "turbo": "1.9.3",
    "typescript": "5.0.4",
    "vite": "4.3.1",
    "vite-plugin-checker": "0.5.6",
    "vitest": "0.30.1"
  }
}
