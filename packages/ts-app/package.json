{
  "name": "@ti/ts",
  "version": "1.0.0",
  "private": true,
  "main": "dist/index.js",
  "scripts": {
    "clean": "rimraf .turbo node_modules dist",
    "lint": "eslint --fix --max-warnings=0 --cache src --ext .ts",
    "dev": "vite --open --port 3000",
    "types": "tsc --noEmit -p .",
    "types:dev": "tsc --noEmit -p . --watch",
    "cd": "turbo types lint test",
    "html": "parcel 'pages/*.html' --lazy"
  },
  "dependencies": {
    "@ti/core": "workspace:*",
    "immer": "10.0.2",
    "remeda": "1.14.0",
    "tiny-invariant": "1.3.1",
    "tiny-warning": "1.0.3",
    "tslib": "2.5.0",
    "zod": "3.21.4"
  },
  "devDependencies": {
    "@faker-js/faker": "8.0.1",
    "@tsconfig/esm": "1.0.3",
    "@tsconfig/strictest": "2.0.1",
    "@types/node": "20.1.7",
    "eslint": "8.40.0",
    "eslint-config-ti": "workspace:*",
    "parcel": "2.8.3",
    "prettier": "2.8.8",
    "turbo": "1.9.7",
    "typescript": "5.0.4",
    "vite": "4.3.7",
    "vite-plugin-checker": "0.6.0"
  }
}
