{
  "name": "hello-web",
  "version": "1.0.0",
  "main": "dist/hello.js",
  "scripts": {
    "lint": "eslint --ext .ts,.tsx src",
    "dev": "vite-node -w src/index.ts",
    "start": "vite-node src/index.ts",
    "test:dev": "vitest",
    "test": "vitest run",
    "types": "tsc --noEmit -p .",
    "types:dev": "tsc --noEmit -p . --watch",
    "prepare": "husky install",
    "ci": "turbo types lint test"
  },
  "dependencies": {
    "ix": "5.0.0",
    "tiny-invariant": "1.3.1",
    "tiny-warning": "1.0.3",
    "tslib": "2.5.0",
    "zod": "3.21.4"
  },
  "devDependencies": {
    "@types/lodash": "4.14.191",
    "@types/node": "18.15.0",
    "@typescript-eslint/eslint-plugin": "5.54.1",
    "@typescript-eslint/parser": "5.54.1",
    "eslint": "8.36.0",
    "eslint-config-prettier": "8.7.0",
    "eslint-plugin-prettier": "4.2.1",
    "husky": "8.0.3",
    "prettier": "2.8.4",
    "turbo": "1.8.3",
    "typescript": "4.9.5",
    "vite": "4.1.4",
    "vite-node": "0.29.2",
    "vitest": "0.29.2"
  }
}
