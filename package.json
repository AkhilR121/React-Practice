{
  "name": "fullstack-training",
  "version": "1.0.0",
  "private": true,
  "main": "dist/index.js",
  "scripts": {
    "lint": "eslint --fix --cache --ext .ts,.tsx src",
    "dev": "vite",
    "test:dev": "vitest",
    "test": "vitest run --passWithNoTests",
    "cov": "vitest run --coverage",
    "cov:dev": "vitest watch --coverage",
    "types": "tsc --noEmit -p .",
    "format": "pnpm prettier --write --ignore-unknown .",
    "types:dev": "tsc --noEmit -p . --watch",
    "prepare": "husky install",
    "cd": "turbo types lint test"
  },
  "dependencies": {
    "immer": "9.0.21",
    "tiny-invariant": "1.3.1",
    "tiny-warning": "1.0.3",
    "tslib": "2.5.0",
    "zod": "3.21.4"
  },
  "devDependencies": {
    "@types/node": "18.15.11",
    "@typescript-eslint/eslint-plugin": "5.57.1",
    "@typescript-eslint/parser": "5.57.1",
    "@vitest/coverage-c8": "0.29.8",
    "eslint": "8.37.0",
    "eslint-config-prettier": "8.8.0",
    "eslint-plugin-prettier": "4.2.1",
    "husky": "8.0.3",
    "lint-staged": "13.2.0",
    "prettier": "2.8.7",
    "turbo": "1.8.8",
    "typescript": "5.0.3",
    "vite": "4.2.1",
    "vite-plugin-checker": "0.5.6",
    "vitest": "0.29.8"
  },
  "lint-staged": {
    "**/*": "pnpm format",
    "**/*.{ts,tsx}": "eslint --fix --cache --ext .ts,.tsx src"
  }
}
