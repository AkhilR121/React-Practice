{
  "name": "fullstack-training",
  "devDependencies": {
    "ndb": "1.1.5",
    "npm-check-updates": "16.10.8",
    "turbo": "1.9.1",
    "@vitest/ui": "latest",
    "typescript": "5.0.4",
    "vitest": "0.30.1"
  },
  "scripts": {
    "upd": "pnpm add -g @pnpm/exe; pnpm ncu -u; pnpm turbo upd",
    "types": "pnpm turbo types",
    "lint": "pnpm turbo lint",
    "build": "pnpm turbo build",
    "test": "pnpm vitest run",
    "test:dev": "pnpm vitest",
    "test:ui": "vitest --ui",
    "clean": "pnpm turbo clean",
    "cd": "pnpm turbo types lint && pnpm test"
  }
}
