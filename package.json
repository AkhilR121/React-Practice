{
  "name": "fullstack-training",
  "devDependencies": {
    "@vitest/ui": "0.31.4",
    "eslint": "8.42.0",
    "prettier": "2.8.8",
    "turbo": "1.10.1",
    "typescript": "5.1.3",
    "vitest": "0.31.4"
  },
  "scripts": {
    "format": "pnpm prettier --write --ignore-unknown .",
    "upd": "pnpm --recursive --latest update",
    "types": "pnpm turbo types",
    "lint": "pnpm turbo lint",
    "build": "pnpm turbo build",
    "test": "pnpm vitest run",
    "test:dev": "pnpm vitest",
    "test:ui": "vitest --ui",
    "clean": "pnpm turbo clean",
    "build:libs": "turbo run build --filter=core --filter=react",
    "cd": "pnpm build:libs && pnpm turbo types lint && pnpm test"
  }
}
